            <kind>1</kind>
            <string>///Save Data
ini_open(global.g_dir+"sys\game.ini")
if (ed_name.str!="")
{
ini_write_string("Player","Name",ed_name.str);
}
var shadows,_max_sounds;
switch (bar_performance.value)
{
case 0:shadows=0;  _max_sounds=10; break;
case 1:shadows=1;  _max_sounds=20; break;
case 2:shadows=1;  _max_sounds=60; break;
}
ini_write_real("Performance","shadows",shadows);
ini_write_real("Performance","chanel_num",_max_sounds);
ini_write_real("Performance","OverAll",bar_performance.value);
ini_close();



room_goto(rm_menu);
